                    <div class="am-g tpl-amazeui-form">

                        <div class="am-u-sm-12 am-u-md-9">
                            <form name="formExchange" method="post" action="usercenter.php" class="am-form am-form-horizontal" onSubmit="return submitExchange()">
                                
                                <div class="am-form-group">
                                    <label for="user-phone" class="am-u-sm-3 am-form-label">重销</label>
                                    <div class="am-u-sm-5">
                                        <input type="text" name="cash" id="cash" placeholder="请输入你要转化的数量">
                                    </div>
                                    <span style="line-height:34px;font-size:16px;color:#999;">
                                        您当前可用重销币为 {$info.user_cash} 个
                                    </span>
                                </div>

                                <div class="am-form-group">
                                    <label for="user-phone" class="am-u-sm-3 am-form-label">企业币</label>
                                    <div class="am-u-sm-5">
                                        <input type="text" name="qiyebi" id="qiyebi" placeholder="请输入你要转化的数量">
                                    </div>
                                    <span style="line-height:34px;font-size:16px;color:#999;">
                                        您当前可用企业币为 {$info.user_point} 个
                                    </span>
                                </div>

                                <div class="am-form-group">
                                    <div class="am-u-sm-9 am-u-sm-push-3">
                                        <input type="submit" name="submit" value="确认转化"  class="am-btn am-btn-primary"/>
                                    </div>
                                </div>
                                <input type="hidden" name="act" value="act_exchange_check" />
                                
                            </form>
                        </div>
                    </div>
                </div>
                </div>
                <div class="tpl-alert"></div>
            </div>  
        </div>
        <script>
            function submitExchange(){
                var frm = document.forms['formExchange'];
                var cash = frm.elements['cash'].value;
                var qiyebi = frm.elements['qiyebi'].value;
                if (cash == '') {
                    alert('请输入您要使用的重销币数量');return false;
                } else {
                    var reg = /^([1-9]\d*|0)(\.\d{1,2})?$/;
                    if (!reg.test(cash)) {
                        alert('您输入的格式不正确');return false;
                    }
                }
                if (cash > {$info.user_cash}){
                    alert('重销币不足');
                    document.getElementById("cash").value=0;return false;
                }
                if (qiyebi == '') {
                    alert('请输入您要使用的企业币数量');return false;
                } else {
                    var reg = /^([1-9]\d*|0)(\.\d{1,2})?$/;
                    if (!reg.test(qiyebi)) {
                        alert('您输入的格式不正确');return false;
                    }
                }
                if (qiyebi > {$info.user_point}){
                    alert('企业币不足');
                    document.getElementById("qiyebi").value=0;return false;
                }
                
            }
        </script>
        {/if}

