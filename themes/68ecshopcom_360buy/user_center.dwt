                }
                if (shengjibi.length == 0) {
                    alert('请输入您要使用的升级币数量');return false;
                } else {
                    var reg = /^([1-9]\d*|0)(\.\d{1,2})?$/;
                    if (!reg.test(shengjibi)) {
                        alert('您输入的升级币格式不正确');return false;
                    }
                }
                if (shengjibi > {$res.user_upgrade}){
                    alert('升级币数量不足');
                    document.getElementById("shengjibi").value=0;return false;
                }
                if({$data.user_rank} == 1){
                    if(eval(shengjibi)+eval(xianjin) != {$pu}){
                        alert('您填写的金额不正确');return false;
                    }
                }else if({$data.user_rank} == 2){
                    if(eval(shengjibi)+eval(xianjin) != {$yin}){
