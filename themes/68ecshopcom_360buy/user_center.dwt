        <script>
            function submitJihuo(){
                var frm = document.forms['formJihuo'];
                var jh = {$info.user_upgrade};
                if({$info.user_rank} == 1){
                    if(jh < {$pu}){
                        alert('激活普卡的激活币不足');return false;
                    }
                }else if({$info.user_rank} == 2){
                    if(jh < {$yin}){
                         alert('激活银卡的激活币不足');return false;                   
                    }
                }else if({$info.user_rank} == 3){
                    if(jh < {$jin}){
                        alert('激活金卡的激活币不足');return false;
                    }
                }else if({$info.user_rank} == 4){
                    if(jh < {$zuan}){
                        alert('激活钻卡的激活币不足');return false;
                    }
                }
                
                
            }
