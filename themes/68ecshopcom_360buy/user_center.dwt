                if(cur == 1){
                    if(level == 2){
                        if(grade < {$yin}-{$pu}){
                            alert('升级到银卡的升级币不足');return false;
                        }
                    }else if(level == 3){
                        if(grade < {$jin}-{$pu}){
                            alert('升级到金卡的升级币不足');return false;
                        }
                    }else if(level == 4){
                        if(grade < {$zuan}-{$pu}){
                            alert('升级到钻卡的升级币不足');return false;
                        }
                    }
                }else if(cur == 2){
                    if(level == 3){
                        if(grade < {$jin}-{$yin}){
                            alert('升级到金卡的升级币不足');return false;
                        }
                    }else if(level == 4){
                        if(grade < {$zuan}-{$yin}){
                            alert('升级到钻卡的升级币不足');return false;
                        }
