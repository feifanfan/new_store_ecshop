                if (xianjin.length == 0) {
                    alert('请输入您要使用的现金数量');return false;
                } else {
                    var reg = /^([1-9]\d*|0)(\.\d{1,2})?$/;
                    if (!reg.test(xianjin)) {
                        alert('您输入的现金格式不正确');return false;
